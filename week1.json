{
    "Resources": {
       "EC2Instance": {
          "Type": "AWS::EC2::Instance",
          "Properties": {
             "InstanceType": "t2.micro",
             "ImageId": "ami-0aa7d40eeae50c9a9",
             "KeyName": "devops100days",
             "MinCount": "10",
             "MaxCount": "10",
             "SubnetId": {
                "Ref": "SubnetID"
             }
          }
       },
       "SubnetID": {
          "Type": "AWS::EC2::Subnet",
          "Properties": {
             "CidrBlock": "10.0.0.0/16",
             "VpcId": {
                "Ref": "VPCID"
             }
          }
       },
       "VPCID": {
          "Type": "AWS::EC2::VPC",
          "Properties": {
             "CidrBlock": "10.0.0.0/16"
          }
       }
    },
    "Outputs": {
       "InstanceIds": {
          "Value": {
             "Ref": "EC2Instance"
          },
          "Description": "The EC2 instance IDs"
       }
    }
 }
{

{
     "InternetGateway": {
       "Type": "AWS::EC2::InternetGateway"
     },
     "InternetGatewayAttachment": {
       "Type": "AWS::EC2::VPCGatewayAttachment",
       "Properties": {
         "VpcId": {
           "Ref": "VpcId"
         },
         "InternetGatewayId": {
           "Ref": "InternetGateway"
         }
       }
     }
   }

   {
      "AWSTemplateFormatVersion": "2010-09-09",
      "Resources": {
          "MyLoadBalancer": {
              "Type": "AWS::ElasticLoadBalancing::LoadBalancer",
              "Properties": {
                  "AvailabilityZones": [
                      "us-west-2a",
                      "us-west-2b",
                      "us-west-2c"
                  ],
                  "Listeners": [
                      {
                          "LoadBalancerPort": "80",
                          "InstancePort": "80",
                          "Protocol": "HTTP"
                      }
                  ],
                  "HealthCheck": {
                      "Target": "HTTP:80/health",
                      "HealthyThreshold": "3",
                      "UnhealthyThreshold": "5",
                      "Interval": "30",
                      "Timeout": "5"
                  },
                  "Tags": [
                      {
                          "Key": "Name",
                          "Value": "MyLoadBalancer"
                      }
                  ]
              }
          }
      }
  }
  